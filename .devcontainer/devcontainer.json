{
  "name": "Go-devcontainer",
  "service": "minecraft-watcher",
  "dockerComposeFile": [
    "../docker-compose-dev.yml"
  ],
  "workspaceFolder": "/go/src/github.com/taiki2523/minecraft-watcher",
  "customizations": {
    "vscode": {
      "settings": {
        "go.gopath": "/go",
        "go.inferGopath": true,
        "go.useLanguageServer": true
      },
      "extensions": [
        "golang.go"
      ]
    }
  },
  "forwardPorts": []
  //"postCreateCommand": "go mod tidy"
}
